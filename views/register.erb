<form action="/register" method="post">
  <%= params["message"] %> <br>
  First name <br> <input type="text" name="fname" value=""><br>
  Last name <br> <input type="text" name="lname" value=""><br>
  Username <br> <input type="text" name="uname" value=""><br>
  Email <br> <input type="email" name="email" value=""><br>
  Password <br> <input type="password" name="password" value=""><br>
  password confirm <br> <input type="password" name="password_con" value=""><br>
  Birthday <br> <input type="date" name="bday" value=""><br>
  Gender <br> <input type="radio" name="sex" value="male" checked>Male <br>
          <input type="radio" name="sex" value="female">Female <br>
  <input type="submit" value="Register">
</form>
